# Home Environment Configuration Template
# Copy this to .env.home and fill in actual values

#######################
# Environment
#######################
PLANE=home
SUBDOMAIN=home
HOME_DOMAIN=home.offhourlab.dev

#######################
# Data Paths
#######################
APP_DATA_ROOT=/home/user/homelab/data/home
APP_LOG_ROOT=/home/user/homelab/logs/home

#######################
# Service Domains
# These are accessed via cloud Traefik
#######################
NEXTCLOUD_DOMAIN=nextcloud.home.offhourlab.dev
IMMICH_DOMAIN=immich.home.offhourlab.dev

#######################
# Tailscale
#######################
TAILSCALE_HOSTNAME=home-lab

#######################
# SECRETS - Fill these in
#######################

# Tailscale - Get from: https://login.tailscale.com/admin/settings/keys
TAILSCALE_AUTHKEY=tskey-auth-REPLACE_ME

# Tailscale IP (fill after first connection)
TAILSCALE_IP=100.x.x.x